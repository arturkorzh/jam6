+using System.Collections.Generic;
+using UnityEngine;
+
+namespace Enitities
+{
+    // several base blocks in one player
+    public class Player : MonoBehaviour
+    {
+        public List<Block> UserInitBlocks;
+        public List<Entity> Collected;
+
+        //center is 0,0
+        public int[,] position = new int[50, 50];
+
+        public float Health;
+        public int Money;
+
+        void Start()
+        {
+            GameState.Player = this;
+        }
+
+        // Update is called once per frame
+        void Update()
+        {
+            GameState.IsCollecting = Input.GetKeyDown("space");
+        }
+    }
+}
