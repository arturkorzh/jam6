+
+public static class GameState
+{
+    // gameObj 
+    public static Player Player { get; set; }
+
+
+    // Affect player movement
+    public static bool IsDead { get; set; }
+    public static bool IsCollecting { get; set; }
+
+
+    public static float CollectingStartTime { get; set; }
+
+    // Resources
+    public static float Points { get; set; }
+    public static float Coins { get; set; }
+}
