+using System;
+using System.Collections;
+using System.Collections.Generic;
+using System.Linq;
+using UnityEngine;
+
+namespace Enitities
+{
+    // tags: UserBlock, ObtainedBlock, FreeBlock
+    public class Block : MonoBehaviour
+    {
+        public BlockProperty Property;
+
+        bool isCurrentlyColliding;
+        private float collectingTime;
+        private Collision currentCollision;
+        public Action Destroyed;
+        public Action<List<Vector3>> Obtained;
+
+        void OnCollisionEnter(Collision collision)
+        {
+            currentCollision = collision;
+            isCurrentlyColliding = true;
+            if (collision.gameObject.CompareTag("Wall") || (collision.gameObject.CompareTag("Enemy") &&
+                                                            !collision.gameObject.CompareTag("FreeBlock")))
+                Destroyed?.Invoke();
+
+
+            if (gameObject.CompareTag("FreeBlock"))
+            {
+                if (collision.gameObject.CompareTag("ObtainedBlock") || collision.gameObject.CompareTag("UserBlock"))
+                {
+                    //Collectors mode on 
+                    if (Input.GetKeyDown("space"))
+                    {
+                        GameState.IsCollecting = true;
+                        collectingTime = Time.time;
+
+                        if (collectingTime == 0)
+                            Debug.Log("Collecting started");
+
+                        collectingTime = Time.time;
+                        if (Math.Abs(Time.time - collectingTime - 100) < 0.01)
+                        {
+                            Obtained?.Invoke(collision.contacts.Select(x => x.point).ToList());
+                            Debug.Log("Collecting finished");
+                        }
+                    }
+                    else //pushing block away
+                    {
+                        // Calculate Angle Between the collision point and the player
+                        var dir = collision.contacts[0].point - transform.position;
+                        // We then get the opposite (-Vector3) and normalize it
+                        dir = -dir.normalized;
+                        // And finally we add force in the direction of dir and multiply it by force. 
+                        // This will push back the block
+                        gameObject.GetComponent<Rigidbody>().AddForce(dir * 3);
+                        Debug.Log("FreeBlock pushed");
+                    }
+                }
+            }
+        }
+
+        void OnCollisionExit(Collision collision)
+        {
+            isCurrentlyColliding = false;
+
+            if (currentCollision == collision &&
+                (gameObject.CompareTag("ObtainedBlock") || gameObject.CompareTag("UserBlock")))
+            {
+                collectingTime = 0;
+                Debug.Log("Collecting refreshed");
+            }
+
+            currentCollision = null;
+        }
+    }
+}
