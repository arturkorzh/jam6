+using System.Collections;
+using System.Collections.Generic;
+using UnityEngine;
+
+namespace Enitities
+{
+    public class Entity : MonoBehaviour
+    {
+        public List<Block> Blocks;
+        private static float RespawnTime = 5;
+        private Vector2 SpawnPoint;
+        public bool isDestroyed;
+        public bool isObtained;
+
+        void Start()
+        {
+            SpawnPoint = gameObject.transform.position;
+            foreach (var block in Blocks)
+            {
+                block.Destroyed += Destroy;
+                block.Obtained += Obtain;
+            }
+        }
+
+        private void Destroy()
+        {
+            if (isDestroyed) return;
+            isDestroyed = true;
+            gameObject.SetActive(false);
+            if (isObtained)
+                
+                GameState.Player.Collected.Remove(this);
+            StartCoroutine(RespawnBlock());
+        }
+
+        private void Obtain(List<Vector3> vector3s)
+        {
+            if (isObtained) return;
+            isObtained = true;
+
+            foreach (var block in Blocks)
+                block.tag = "ObtainedBlock";
+
+            GameState.Player.Collected.Add(this);
+        }
+
+        public IEnumerator RespawnBlock()
+        {
+            yield return new WaitForSeconds(RespawnTime);
+            gameObject.SetActive(true);
+            gameObject.transform.position = new Vector3(SpawnPoint.x, SpawnPoint.y, 0);
+            foreach (var block in Blocks)
+                block.tag = "FreeBlock";
+        }
+    }
+}
